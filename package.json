{
  "name": "attasist",
  "version": "0.0.22",
  "description": "A library of helper functions for front-end and back-end JavaScript applications",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "debug": "nodemon test/index.js --watch lib --watch test --inspect --quiet --exec 'clear && npm run test'",
    "start": "nodemon test/index.js --watch lib --watch test --quiet --exec 'clear && npm run test'",
    "clean": "rimraf dist es coverage",
    "lint": "eslint lib test",
    "test": "cross-env BABEL_ENV=commonjs babel-node test | faucet",
    "generate-docs": "jsdoc -c jsdoc.json",
    "prepublish": "npm run build",
    "prebuild": "npm run clean && npm run lint && npm test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel lib --out-dir cjs",
    "build:es": "cross-env BABEL_ENV=es babel lib --out-dir es",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -i lib/index.js -o dist/attasist.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -i lib/index.js -o dist/attasist.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Attainia/attasist.git"
  },
  "keywords": [
    "attainia",
    "helpers",
    "assist",
    "nodejs",
    "javascript",
    "react",
    "redux",
    "ramda",
    "helper",
    "utils",
    "functions",
    "functional",
    "programming"
  ],
  "license": "Apache-2.0",
  "author": "Attainia Development <developers@attainia.com>",
  "contributors": [
    "Alex <alex.maskovyak@attainia.com>",
    "Ana <ana.tomboulian@attainia.com>",
    "David Nunez <david.nunez@attainia.com>",
    "Jono <jono.mercier@attainia.com>",
    "Glenn <glenn.siegman@attainia.com>",
    "Marcos <marcos.gonzales@attainia.com>",
    "Michael <michael.tomcal@attainia.com>",
    "Sean McHugh <sean.mchugh@attainia.com>",
    "Tim <tim.graf@attainia.com>",
    "Lawrence <lawrence.grant@attainia.com>"
  ],
  "dependencies": {
    "docdash": "github:Attainia/docdash#master",
    "qs": "6.5.1",
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-check-es2015-constants": "6.3.13",
    "babel-plugin-ramda": "1.4.3",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-es2015-arrow-functions": "6.3.13",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.3.13",
    "babel-plugin-transform-es2015-block-scoping": "6.26.0",
    "babel-plugin-transform-es2015-classes": "6.3.13",
    "babel-plugin-transform-es2015-computed-properties": "6.3.13",
    "babel-plugin-transform-es2015-destructuring": "6.3.13",
    "babel-plugin-transform-es2015-for-of": "6.3.13",
    "babel-plugin-transform-es2015-function-name": "6.3.13",
    "babel-plugin-transform-es2015-literals": "6.3.13",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-es2015-object-super": "6.3.13",
    "babel-plugin-transform-es2015-parameters": "6.3.13",
    "babel-plugin-transform-es2015-shorthand-properties": "6.3.13",
    "babel-plugin-transform-es2015-spread": "6.3.13",
    "babel-plugin-transform-es2015-sticky-regex": "6.3.13",
    "babel-plugin-transform-es2015-template-literals": "6.3.13",
    "babel-plugin-transform-es2015-unicode-regex": "6.3.13",
    "babel-plugin-transform-es3-member-expression-literals": "6.5.0",
    "babel-plugin-transform-es3-property-literals": "6.5.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-register": "6.26.0",
    "babel-tape-runner": "2.0.1",
    "cross-env": "5.1.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb-base": "11.3.2",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.2.0",
    "faucet": "0.0.1",
    "glob": "7.1.1",
    "jsdoc": "3.5.5",
    "nodemon": "1.11.0",
    "rimraf": "2.6.2",
    "rollup": "0.50.0",
    "rollup-plugin-async": "1.2.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "tape": "4.8.0",
    "uglify-es": "3.3.9"
  },
  "prettier": {
    "bracketSpacing": false,
    "parser": "babylon",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
  }
}
